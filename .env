# GyroDiagnostics Environment Configuration
# This file configures Inspect AI for the VS Code extension

# Local Model Configuration
# Using Qwen3-1.7B-Base as both evaluated model and judge
INSPECT_EVAL_MODEL=hf/local
INSPECT_EVAL_MODEL_ARGS=model_path=D:\ML\hf_cache\models--Qwen--Qwen3-1.7B-Base

# Log Configuration
INSPECT_LOG_DIR=./logs
INSPECT_LOG_LEVEL=info
INSPECT_LOG_FORMAT=eval

# Evaluation Defaults
INSPECT_EVAL_MAX_RETRIES=3
INSPECT_EVAL_MAX_CONNECTIONS=2

# File Naming Pattern (includes model name in log files)
INSPECT_EVAL_LOG_FILE_PATTERN={task}_{model}_{id}